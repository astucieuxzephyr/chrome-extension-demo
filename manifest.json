{
	"manifest_version": 2,
	"name": "Extension Tanguy",
	"version": "0.0.1", 

	"description": "Extension Chrome (par Tanguy Bodin-Hullin)",
	"default_locale": "fr",
	"icons": {
		"16": "user-avatar-main-picture-16.png",
		"128": "user-avatar-main-picture-128.png",
		"256": "user-avatar-main-picture-256.png"
	},
	"background": {
		"persistent": true,
		"scripts": [
			"background.js"
		]
	},
	"browser_action" :
	{
		"default_title": "Quizz Psycho !!",
		"default_icon": "user-avatar-main-picture-256.png",
		"default_popup" :"popup_quizz_fr.html"
	},
	"permissions": [
		"tabs",
		"background",
		"contextMenus",
		"notifications",
		"activeTab"
	],

	"author": "Tanguy BODIN-HULLIN"
}
